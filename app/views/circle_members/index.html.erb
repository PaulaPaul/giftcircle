<h1>Listing circle_members</h1>

<table>
  <thead>
    <tr>
      <th>Recipient</th>
      <th>Giver nickname</th>
      <th>Giver</th>
      <th>Receiver Interests</th>
      <th>Giver Notes</th>
      <th>Giftgiven?</th>
      <th>Thankyou</th>
      <th>Giftcircle</th>
      <th>Gift</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @circle_members.each do |circle_member| %>
      <tr>
        <td><%= circle_member.receiver_id %></td>
        <td><%= circle_member.giverNickname %></td>
        <td><%= circle_member.person_id %></td>
        <td><%= circle_member.personInterests %></td>
        <td><%= circle_member.giverNotes %></td>
        <td><%= circle_member.giftGiven? %></td>
        <td><%= circle_member.thankYou %></td>
        <td><%= circle_member.giftCircle_id %></td>
        <td><%= circle_member.gift_id %></td>
        <td><%= link_to 'Show', circle_member %></td>
        <td><%= link_to 'Edit', edit_circle_member_path(circle_member) %></td>
        <td><%= link_to 'Destroy', circle_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Circle member', new_circle_member_path %>
